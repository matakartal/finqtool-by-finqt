  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.56.0",
