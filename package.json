{
  "name": "finqt-extension",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run post-build",
    "build:dev": "vite build --mode development && npm run post-build",
    "build:prod": "NODE_ENV=production vite build && npm run post-build",
    "post-build": "node -e \"console.log('Build completed successfully')\"",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
